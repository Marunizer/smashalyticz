{
 "name": "smashalyticz",
 "author": "marunizer <mendez1226@gmail.com>",
 "repository": "https://github.com/Marunizer/smashalyticz.git",
 "license": "MIT",
 "version": "0.9.6",
 "private": false,
 "dependencies": {
  "concurrently": "^4.1.0",
  "cross-env": "^5.1.4",
  "electron-is-dev": "^1.0.1",
  "electron-log": "^2.2.17",
  "electron-packager": "^12.2.0",
  "electron-updater": "^4.0.4",
  "react": "^16.6.3",
  "react-dom": "^16.6.3",
  "react-redux": "^5.1.1",
  "react-router-dom": "^4.3.1",
  "react-scripts": "2.1.1",
  "redux": "^4.0.1",
  "wait-on": "^3.2.0"
 },
 "devDependencies": {
  "electron": "^3.0.10",
  "electron-builder": "^20.36.2"
 },
 "scripts": {
  "dev": "yarn react-scripts start",
  "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
  "build": "react-scripts build",
  "test": "react-scripts test",
  "eject": "react-scripts eject",
  "pack": "electron-builder --dir",
  "dist": "electron-builder --x64  --win -c.extraMetadata.main=build/electron.js",
  "postinstall": "electron-builder install-app-deps",
  "gh-publish": "electron-builder --x64  --win --c.extraMetadata.main=build/electron.js -p always"
 },
 "description": "best app NA",
 "main": "public/electron.js",
 "homepage": "./",
 "keywords": [
  "Example",
  "React",
  "Electron"
 ],
 "eslintConfig": {
  "extends": "react-app"
 },
 "browserslist": [
  ">0.2%",
  "not dead",
  "not ie <= 11",
  "not op_mini all"
 ],
 "build": {
  "publish": [
   {
    "provider": "github",
    "owner": "marunizer",
    "repo": "smashalyticz"
   }
  ],
  "appId": "com.marunizer.smashalyticz",
  "compression": "normal",
  "productName": "Smashalyticz",
  "artifactName": "${productName}.${ext}",
  "directories": {
   "buildResources": "build",
   "output": "dist"
  },
  "mac": {
   "category": "public.app-category.productivity",
   "icon": "assets/icons/mac/icon.icns",
   "type": "distribution",
   "target": [
    "pkg",
    "dmg",
    "mas"
   ]
  },
  "mas": {
   "entitlements": "assets/entitlements.mas.plist",
   "entitlementsInherit": "assets/entitlements.mas.inherit.plist",
   "provisioningProfile": "assets/embedded.provisionprofile"
  },
  "win": {
   "target": "nsis-web",
   "icon": "assets/icons/win/icon.ico"
  },
  "nsisWeb": {
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "perMachine": true,
    "oneClick": false,
    "allowToChangeInstallationDirectory": true,
    "createDesktopShortcut": true,
    "differentialPackage": true
},
  "linux": {
   "icon": "assets/icons/png/icon.png",
   "target": [
    "snap",
    "AppImage"
   ],
   "description": "best app",
   "category": "Network;Feed"
  }
 }
}
